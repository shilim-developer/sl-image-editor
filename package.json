{
  "name": "sl-image-editor",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc && vite build",
    "preview": "vite preview",
    "format:code": "prettier --write \"(src|build)/**/*.(vue|js)\"",
    "commit": "git-cz",
    "prepare": "husky"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-git",
      "useEmoji": true
    }
  },
  "dependencies": {
    "@cyhnkckali/vue3-color-picker": "^2.0.2",
    "@vicons/ionicons5": "^0.12.0",
    "@vueuse/components": "^10.11.0",
    "@vueuse/core": "^10.9.0",
    "axios": "^1.7.2",
    "css-to-mat": "^1.1.1",
    "fabric": "^5.3.0",
    "less": "^4.2.0",
    "lodash": "^4.17.21",
    "mockjs": "^1.1.0",
    "naive-ui": "^2.38.2",
    "pinia": "^2.1.7",
    "uuid": "^10.0.0",
    "vite-plugin-mock": "^3.0.2",
    "vue": "^3.4.19",
    "vue-router": "^4.3.0",
    "vue3-color-picker-gradient": "^0.7.0",
    "vue3-moveable": "^0.28.0",
    "vue3-selecto": "^1.12.3"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.3.0",
    "@commitlint/config-conventional": "^19.2.2",
    "@types/fabric": "^5.3.7",
    "@types/lodash": "^4.17.5",
    "@types/node": "^20.11.20",
    "@vitejs/plugin-vue": "^5.0.4",
    "commitizen": "^4.3.0",
    "commitlint-config-cz": "^0.13.3",
    "cz-customizable": "^7.1.0",
    "cz-git": "^1.9.3",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.7",
    "prettier": "^3.3.2",
    "typescript": "^5.2.2",
    "unocss": "^0.58.5",
    "unplugin-auto-import": "^0.17.5",
    "unplugin-vue-components": "^0.26.0",
    "vite": "^5.1.4",
    "vue-tsc": "^1.8.27"
  },
  "lint-staged": {
    "*.js": [
      "prettier --write"
    ],
    "*.ts": [
      "prettier --write"
    ],
    "*.tsx": [
      "prettier --write"
    ],
    "*.vue": [
      "prettier --parser=vue --write"
    ],
    "*.css": [
      "prettier --write"
    ],
    "*.scss": [
      "prettier --write"
    ]
  }
}
